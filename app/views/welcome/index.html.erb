<% if params[:campaign].present? %>
  <% if params[:campaign] == "Shelbys-fund" %>
    <% cookies[:campaign] = "Shelbys-fund" %>
    <%= render partial: "welcome/Shelbys-fund" %>

  <% elsif params[:campaign] == "Saras-fund" %>
    <% cookies[:campaign] = "Saras-fund" %>
    <%= render partial: "welcome/Saras-fund" %>

  <% elsif params[:campaign] == "King-Kell-fund" %>
    <% cookies[:campaign] = "King-Kell-fund" %>
    <%= render partial: "welcome/King-Kell-fund" %>

  <% elsif params[:campaign] == "Tony-Rojas-fund" %>
    <% cookies[:campaign] = "Tony-Rojas-fund" %>
    <%= render partial: "welcome/Tony-Rojas-fund" %>

  <% elsif params[:campaign] == "Tony-Rojas-copy-fund" %>
    <% cookies[:campaign] = "Tony-Rojas-copy-fund" %>
    <%= render partial: "welcome/Tony-Rojas-copy-fund" %>
  
  <% elsif params[:campaign] == "stl-girlscout-fund" %>
    <% cookies[:campaign] = "stl-girlscout-fund" %>
    <%= render partial: "welcome/stl-girlscout-fund" %>

  <% end %>
<% else %>
  <%= render partial: "welcome/default" %>
<% end %>


