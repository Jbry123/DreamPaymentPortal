<% if params[:compaign].present? %>
  <% if params[:compaign] == "shelbys-fund" %>
    <% cookies[:compaign] = "shelbys-fund" %>
    <%= render partial: "welcome/shelbys-fund" %>
  <% elsif params[:compaign] == "saras-fund" %>
    <% cookies[:compaign] = "Saras-fund" %>
    <%= render partial: "welcome/saras-fund" %>
  <% end %>
<% else %>
  <%= render partial: "welcome/default" %>
<% end %>


